#2023-03-23 4주차 C언어 수업

#include <stdio.h>
#include <stdlib.h>

int main()
{
    //srand((unsigned)time(NULL));
    int answer = rand()%100;
    int guess;
    int tries=0;
    do
    {
        printf("정답을 추측하여 보시오:");
        scanf("%d", &guess);
        tries++;
        if (guess>answer)
        printf("HIGH\n");
        if (guess<answer)
        printf("LOW\n");
    }
    while (guess != answer);

    printf("축하합니다. 시도횟수=%d\n", tries);
    return 0;
    
}

